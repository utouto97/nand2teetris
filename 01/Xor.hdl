/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a, out=x);
    Not(in=b, out=y);
    And(a=a, b=y, out=s);
    And(a=x, b=b, out=t);
    Or(a=s, b=t, out=out);
}